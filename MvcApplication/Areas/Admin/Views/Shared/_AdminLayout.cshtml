<!DOCTYPE html>
<html>
@*<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">*@
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Admin - @ViewBag.Title</title>
    <link href='https://fonts.googleapis.com/css?family=Cabin+Condensed:400,700' rel='stylesheet' type='text/css' />
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:600,400,300' rel='stylesheet' type='text/css' />
    <link href="@Url.Content("/Areas/Admin/Content/css/style.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("/Areas/Admin/Content/css/font-awesome.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("/Areas/Admin/Content/chosen/chosen.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("/Areas/Admin/Content/css/jquery.ui.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("/Areas/Admin/Content/css/google-like-buttons.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("/Areas/Admin/Content/css/jquery.fancybox.css")" rel="stylesheet" type="text/css" />
    <!--Datatables Themeroller CDN (Microsoft)-->
    <link rel="stylesheet" type="text/css" href="https://ajax.aspnetcdn.com/ajax/jquery.dataTables/1.9.4/css/jquery.dataTables_themeroller.css" />

    <!--Jquery Themes UI CDN (Microsoft), Visit here to change: www.asp.net/ajaxlibrary/CDNjQueryUI1103.ashx -->
    <link rel="stylesheet" type="text/css" href="https://ajax.aspnetcdn.com/ajax/jquery.ui/1.10.3/themes/pepper-grinder/jquery-ui.css" />

    @RenderSection("style", required: false)

    @*<!--Jquery CDN (Microsoft)-->
    <!-- visit: www.asp.net/ajaxlibrary/cdn.ashx#jQuery_UI_Releases_on_the_CDN_2 -->
    <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-1.9.0.min.js" type="text/javascript"></script>

    <!--Jquery UI CDN (Microsoft)-->
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.ui/1.10.3/jquery-ui.js" type="text/javascript"></script>
    <script type="text/javascript">
        // Fallback to loading jQuery from a local path if the CDN is unavailable
        (window.jQuery || document.write('<script src="/Areas/Admin/Content/js/jquery-1.9.0.min.js"><\/script>'));
    </script>*@
</head>
<body>
    <div class="wrapper">
        <div class="header">
            <div class="logo">
                VIDYANJALI<a href="#" class="sidebar-hook"><i class="fa fa-align-justify fa-2x"></i></a>
            </div>
            <div class="identity">
                @if (User.Identity.IsAuthenticated)
                {
                    <text>Welcome </text>
                    @User.Identity.Name
                    <text>!</text>
                    <a href="@Url.Action("LogOff", "AdminAccount", new { area = "Admin" })">Log Out</a>
                }
            </div>
        </div>
     
            @{
                //Html.RenderPartial("_MainMenu");
                Html.RenderPartial("~/Areas/Admin/Views/Shared/_MainMenu.cshtml");
            } 
      
        <div class="main-content-container">
            @RenderBody()
        </div>
    </div>
    <script src="https://code.jquery.com/jquery-1.10.2.js" type="text/javascript"></script>
    <script src="https://code.jquery.com/ui/1.10.4/jquery-ui.js" type="text/javascript"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js" type="text/javascript"></script>
    <script type="text/javascript" charset="utf8" src="https://ajax.aspnetcdn.com/ajax/jquery.dataTables/1.9.4/jquery.dataTables.min.js"></script>
    <script src="@Url.Content("/Areas/Admin/Content/datatables/js/ColReorder.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("/Areas/Admin/Content/datatables/js/ZeroClipboard.js")" type="text/javascript"></script>
    <script src="@Url.Content("/Areas/Admin/Content/datatables/js/TableTools.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("/Areas/Admin/Content/chosen/chosen.jquery.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("/Areas/Admin/Content/js/jquery.hoverIntent.minified.js")" type="text/javascript"></script>
    <script src="@Url.Content("/Areas/Admin/Content/js/jquery.fancybox.js")" type="text/javascript"></script>
    <script src="@Url.Content("/Areas/Admin/Content/js/admin.js")" type="text/javascript"></script>
     @RenderSection("script", required: false)
    @RenderSection("js", required: false) 
</body>
</html>

