@model Vidyanjali.Areas.Admin.Models.Stakeholder.AdminUser
@{
    ViewBag.Title = "Admin Login";
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, intial-scale=1.0">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <link href="https://fonts.googleapis.com/css?family=Raleway|Titillium+Web" rel="stylesheet">

    <link rel="stylesheet" href="/Content/Public/css/custom.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css" ">
    <title>Vidyanjali</title>
    <style>
        body {
            margin: 0;
            height: 100%;
            overflow: hidden
        }
      </style>
</head>
  <body>
      <div class="outer-container admin-bred-bckgnd">
          <div class="container">
              <div class="row">
                  <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                      <div class="logo-container" style="display: flex;justify-content: center;">
                          <a href="/"> <img src="/content/public/images/Vidyanjali_Logo.webp" class="img-fluid" alt="logo"></a>
                      </div>
                  </div>
              </div>
          </div>
      </div>


      <div class="outer-container outer-wrapper" style="margin-bottom:150px">
          <div class="container">
              <div class="wrapper">
                  <div class="col-lg-6 col-lg-offset-3 col-md-6 col-md-offset-3 col-sm-12 col-xs-12">
                      <div class="card-body enq-frm" data-aos="fade-down" data-aos-easing="linear" data-aos-duration="1800">
                          <h3 class="text-center">Admin Login</h3>
                      </div>
                          <hr class="hr-line">
                          @{
                              Html.RenderPartial("~/Areas/Admin/Views/Shared/Account/_LogOn.cshtml", Model);
                          }
                      </div>
                  </div>
              </div>
          </div>



     
      <div class="outer-container">
          <div class="footer-data">
              <div class="container">
                  <div class="row">
                      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                          <div class="rights">
                              <p>© @DateTime.Now.Year Sri Ramaiah Reddy Educational Trust. All Rights Reserved.</p>
                          </div>
                      </div>
                          <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                              <div class="design">
                                  <p>Design: <a href="lumos.in"> LUMOS.in</a></p>
                              </div>
                          </div>
                      </div>
              </div>
          </div>
      </div>

      <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
      <script type="text/javascript" src="/Content/Public/js/bootstrap-dropdownhover.min.js"></script>
      <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
      <script type="text/javascript" src="/Content/Public/js/validate.js"></script>
      <script type="text/javascript" src="/Content/Public/js/unobrustive.js"></script>
      <script type="text/javascript" src="/Content/Public/js/custom.js"></script>
  </body>
</html>
